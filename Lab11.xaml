<Page x:Class="Lab10_11.Lab11"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:Lab10_11"
      mc:Ignorable="d"
      Title="Lab11" Height="450" Width="800">
	<Grid Margin="10">
		<StackPanel>
			<TextBlock Text="Введите числа через пробел:" FontWeight="Bold" Margin="5"/>
			<TextBox x:Name="InputTextBox" Height="30" Margin="5"/>
			<Button Content="Обработать" Click="ProcessNumbers" Margin="5"/>
			<TextBlock x:Name="BeforeChangeText" Margin="5"/>
			<TextBlock x:Name="AfterChangeText" Margin="5"/>
		</StackPanel>
	</Grid>
</Page>

